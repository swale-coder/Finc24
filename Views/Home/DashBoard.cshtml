<script>


    $('.more').click(function (event) {
        var url = '@Url.Action("ViewAnimation")';
        var id = $(this).parent().find('.anim_Id').val();
        var type = $(this).parent().find('.anim_type').val();
        var imgCount = $(this).parent().find('.anim_imgcount').val();

        $.ajax({
            type: "GET",
            url: url,
            data: JSON,
            cache: false,
            data: {
                type: type,
                id: id,
                imgCount: imgCount
            },
            success: function (data) {
                //  alert(data);
                // inject your content into the "placeholder" div
                $('#myModal').html(data);
            },
            error: function (err) {
                // alert(err);
                console.log(err);
            }
        });
    })

    var val = '@Session["FirstPaymentStatus"]';
    if (val == "Success") {
        $("#myModalPopUp").modal();
    }



</script>
